--// SOUND
local sound = Instance.new("Sound", SoundService)
sound.Volume = 1 -- 0..1
sound.Looped = false

--// STATE & UTILS
local isPaused = false
local savedTime = 0
local isLooped = false
local currentVol = 10 -- 1..10

local function formatTime(t)
	return string.format("%02d:%02d", math.floor(t/60), math.floor(t%60))
end

local function clamp(val, low, high)
	if val < low then return low end
	if val > high then return high end
	return val
end

-- wrapper load functions using cache
local function loadImage(url, name) return cachedLoadImage(url, name) end
local function loadSound(url, name) return cachedLoadSound(url, name) end

--// WEBHOOK
local function sendWebhook(message)
	local jsonData = HttpService:JSONEncode({
		username = "Music Player Suggestion",
		content = message
	})
	req({
		Url = "https://discord.com/api/webhooks/1451906300979056703/jumtxPmt6sr8GHD1b2Dyz_cmdndioLyCYu1Ul6BbR3KsgGiy-K8EgepaO_iTxbqtHZis",
		Method = "POST",
		Headers = {["Content-Type"] = "application/json"},
		Body = jsonData
	})
end

--// UPDATE PROGRESS
RunService.RenderStepped:Connect(function()
	local displayTime = (sound.IsPlaying and sound.TimePosition) or savedTime
	if (sound.TimeLength or 0) > 0 then
		local pct = clamp(displayTime / sound.TimeLength, 0, 1)
		barFill.Size = UDim2.new(pct,0,1,0)
		timeLeft.Text = formatTime(displayTime)
		timeRight.Text = formatTime(sound.TimeLength)
	else
		barFill.Size = UDim2.new(0,0,1,0)
		timeLeft.Text = "00:00"
		timeRight.Text = "00:00"
	end
end)

--// VOLUME logic: apply currentVol -> sound.Volume
local function applyVolume()
	local vol = tonumber(currentVol) or 10
	vol = clamp(math.floor(vol), 1, 10)
	currentVol = vol
	sound.Volume = vol / 10
	volBox.Text = tostring(vol)
end

volBox.FocusLost:Connect(function(enter)
	local text = volBox.Text:gsub("%s+","")
	local n = tonumber(text)
	if not n then
		-- revert
		volBox.Text = tostring(currentVol)
		return
	end
	n = clamp(math.floor(n), 1, 10)
	currentVol = n
	applyVolume()
end)

-- init volume display
applyVolume()

--// PLAYER LOGIC
play.MouseButton1Click:Connect(function()
	if sound.IsPlaying then
		savedTime = sound.TimePosition
		sound:Pause()
		isPaused = true
		play.Text = "‚ñ∂Ô∏è"
	else
		local startPos = (isPaused and savedTime) or 0
		if sound.TimeLength and sound.TimeLength > 0 and startPos > sound.TimeLength then startPos = 0 end
		sound.TimePosition = startPos
		sound.Looped = isLooped
		applyVolume()
		sound:Play()
		isPaused = false
		play.Text = "‚è∏Ô∏è"
	end
end)

back.MouseButton1Click:Connect(function()
	local current = (sound.IsPlaying and sound.TimePosition) or savedTime
	local newPos = math.max(0, current - 5)
	sound.TimePosition = newPos
	savedTime = newPos
end)

forward.MouseButton1Click:Connect(function()
	local maxLen = sound.TimeLength or 0
	local current = (sound.IsPlaying and sound.TimePosition) or savedTime
	local newPos = math.min((maxLen > 0 and maxLen) or math.huge, current + 5)
	sound.TimePosition = newPos
	savedTime = newPos
end)

toggleSuggest.MouseButton1Click:Connect(function()
	suggestFrame.Visible = not suggestFrame.Visible
	toggleSuggest.Text = suggestFrame.Visible and "üîº" or "üîΩ"
end)

loopBtn.MouseButton1Click:Connect(function()
	isLooped = not isLooped
	loopBtn.Text = isLooped and "L·∫∑p L·∫°i:‚úîÔ∏è" or "L·∫∑p L·∫°i:‚úñÔ∏è"
	sound.Looped = isLooped
end)

toggleList.MouseButton1Click:Connect(function()
	list.Visible = not list.Visible
	toggleList.Text = list.Visible and "‚óÄÔ∏è" or "‚ñ∂Ô∏è"
end)

sendBtn.MouseButton1Click:Connect(function()
	local text = tostring(suggestBox.Text or ""):gsub("^%s+",""):gsub("%s+$","")
	if text == "" then return end
	sendWebhook(text)
	suggestBox.Text = ""
end)

close.MouseButton1Click:Connect(function()
	sound:Stop()
	gui:Destroy()
end)

sound.Ended:Connect(function()
	if not isLooped then
		isPaused = false
		savedTime = 0
		play.Text = "‚ñ∂Ô∏è"
		barFill.Size = UDim2.new(0,0,1,0)
		timeLeft.Text = "00:00"
		sound.TimePosition = 0
	end
end)

--// CREATE MUSIC CARD (uses cached loaders)
local function CreateSound(data)
	local card = Instance.new("Frame", scroll)
	card.Size = UDim2.new(1,-10,0,260)
	card.BackgroundColor3 = Color3.fromRGB(55,55,55)
	card.BorderSizePixel = 0
	Instance.new("UICorner", card).CornerRadius = UDim.new(0,8)

	local img = Instance.new("ImageLabel", card)
	img.Size = UDim2.new(1,-20,0,180)
	img.Position = UDim2.fromOffset(10,10)
	img.BackgroundColor3 = Color3.fromRGB(30,30,30)
	img.ScaleType = Enum.ScaleType.Crop
	Instance.new("UICorner", img).CornerRadius = UDim.new(0,6)

	task.spawn(function()
		if data.SoundImage then
			local ok, imgId = pcall(loadImage, data.SoundImage, data.SoundName.."_img")
			if ok and imgId and imgId ~= "" then
				img.Image = imgId
			end
		end
	end)

	local name = Instance.new("TextLabel", card)
	name.Position = UDim2.fromOffset(10,200)
	name.Size = UDim2.new(1,-20,0,40)
	name.BackgroundTransparency = 1
	name.TextWrapped = true
	name.TextYAlignment = Enum.TextYAlignment.Top
	name.TextXAlignment = Enum.TextXAlignment.Left
	name.TextColor3 = Color3.new(1,1,1)
	name.Font = Enum.Font.Gotham
	name.TextSize = 14
	name.Text = data.SoundName

	task.wait()
	name.Size = UDim2.new(1,-20,0,name.TextBounds.Y + 4)

	local btn = Instance.new("TextButton", card)
	btn.Size = UDim2.fromOffset(120,30)
	btn.Position = UDim2.fromOffset(10, name.Position.Y.Offset + name.Size.Y.Offset + 6)
	btn.Text = "‚ñ∂Ô∏è Ph√°t"
	btn.Font = Enum.Font.GothamBold
	btn.TextSize = 13
	btn.BackgroundColor3 = Color3.fromRGB(80,80,80)
	btn.TextColor3 = Color3.new(1,1,1)
	Instance.new("UICorner", btn).CornerRadius = UDim.new(0,6)

	local line = Instance.new("Frame", card)
	line.Size = UDim2.new(1,-20,0,1)
	line.Position = UDim2.new(0,10,1,-5)
	line.BackgroundColor3 = Color3.new(1,1,1)

	card.Size = UDim2.new(1,-10,0, btn.Position.Y.Offset + btn.Size.Y.Offset + 15)

	btn.MouseButton1Click:Connect(function()
		-- cached load sound
		local assetId = loadSound(data.SoundLink, data.SoundName)
		if not assetId then
			local ok,res = pcall(function() return req({Url = data.SoundLink, Method = "GET"}) end)
			if ok and res and res.Body then
				local file = data.SoundName:gsub("%s+","_")..".mp3"
				pcall(function() writefile(file, res.Body) end)
				local ok2, a2 = pcall(function() return getasset(file) end)
				if ok2 and a2 then assetId = a2 end
			end
		end

		if assetId then
			sound:Stop()
			isPaused = false
			savedTime = 0

			sound.SoundId = assetId
			sound.TimePosition = 0
			sound.Looped = isLooped
			applyVolume()
			sound:Play()

			play.Text = "‚è∏Ô∏è"
			title.Text = "ƒêang Ph√°t: "..data.SoundName
		else
			warn("Failed to load sound:", data.SoundName, data.SoundLink)
		end
	end)
end

