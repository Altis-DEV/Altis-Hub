-- FULL MUSIC PLAYER (all original features + loop toggle + UIScale = 1 + top bar with copy buttons + caching + volume textbox)
--// SERVICES
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local SoundService = game:GetService("SoundService")
local HttpService = game:GetService("HttpService")
local LocalPlayer = Players.LocalPlayer

--// EXECUTOR API (helpers)
local req = request or http_request or syn.request
local getasset = getsynasset or getcustomasset
local writefile = writefile
local isfile = isfile
local isfolder = isfolder
local makefolder = makefolder
local readfile = readfile

assert(req and getasset and writefile, "Executor missing request / asset / writefile")

--// CACHE SETUP
local CACHE_FOLDER = "musicplayer_cache"

local function ensureFolder(path)
	if makefolder and isfolder then
		if not isfolder(path) then
			pcall(function() makefolder(path) end)
		end
	end
end

local function fileExists(path)
	if isfile then
		return isfile(path)
	end
	local ok = pcall(function() readfile(path) end)
	return ok
end

ensureFolder(CACHE_FOLDER)

local function sanitize(name)
	name = tostring(name or ""):gsub("%s+", "_")
	name = name:gsub("[^%w%-%_%.]", "")
	if name == "" then name = "file" end
	return name
end

local function cachedLoadImage(url, name)
	if not url then return "" end
	local safeName = sanitize(name)
	local filename = safeName .. ".png"
	local path = filename
	if isfolder then path = CACHE_FOLDER .. "/" .. filename end

	if fileExists(path) then
		local ok, res = pcall(function() return getasset(path) end)
		if ok and res then return res end
	end

	local ok, res = pcall(function() return req({Url = url, Method = "GET"}) end)
	if not ok or not res or not res.Body then return "" end

	local success, err = pcall(function() writefile(path, res.Body) end)
	if not success then
		pcall(function() writefile(filename, res.Body) end)
		local ok2, res2 = pcall(function() return getasset(filename) end)
		if ok2 and res2 then return res2 end
		return ""
	end

	local ok2, asset = pcall(function() return getasset(path) end)
	if ok2 and asset then return asset end
	local ok3, asset3 = pcall(function() return getasset(filename) end)
	if ok3 and asset3 then return asset3 end
	return ""
end

local function cachedLoadSound(url, name)
	if not url then return nil end
	local safeName = sanitize(name)
	local filename = safeName .. ".mp3"
	local path = filename
	if isfolder then path = CACHE_FOLDER .. "/" .. filename end

	if fileExists(path) then
		local ok, res = pcall(function() return getasset(path) end)
		if ok and res then return res end
	end

	local ok, res = pcall(function() return req({Url = url, Method = "GET"}) end)
	if not ok or not res or not res.Body then return nil end

	local success = pcall(function() writefile(path, res.Body) end)
	if not success then
		pcall(function() writefile(filename, res.Body) end)
		local ok2, asset2 = pcall(function() return getasset(filename) end)
		if ok2 and asset2 then return asset2 end
		return nil
	end

	local ok2, asset = pcall(function() return getasset(path) end)
	if ok2 and asset then return asset end
	local ok3, asset3 = pcall(function() return getasset(filename) end)
	if ok3 and asset3 then return asset3 end
	return nil
end

