--// COPY BUTTONS function
local function makeCopyButton(parent, posOffsetX, textLabel, copyText)
	local btn = Instance.new("TextButton", parent)
	btn.Size = UDim2.fromOffset(120,28)
	btn.Position = UDim2.new(1, posOffsetX, 0, 6)
	btn.AnchorPoint = Vector2.new(1,0)
	btn.Text = textLabel
	btn.Font = Enum.Font.GothamBold
	btn.TextSize = 12
	btn.BackgroundColor3 = Color3.fromRGB(70,70,70)
	btn.TextColor3 = Color3.new(1,1,1)
	btn.AutoButtonColor = true

	local fb = Instance.new("TextLabel", parent)
	fb.Size = UDim2.fromOffset(80,20)
	fb.Position = UDim2.new(1, posOffsetX - 130, 0, 8)
	fb.AnchorPoint = Vector2.new(1,0)
	fb.BackgroundTransparency = 1
	fb.Text = ""
	fb.Font = Enum.Font.Gotham
	fb.TextSize = 12
	fb.TextColor3 = Color3.fromRGB(160,255,160)

	btn.MouseButton1Click:Connect(function()
		local ok, _ = pcall(function()
			if setclipboard then
				setclipboard(copyText)
			elseif toclipboard then
				toclipboard(copyText)
			else
				error("no clipboard function")
			end
		end)
		if ok then
			fb.Text = "copied"
			delay(1.2, function() fb.Text = "" end)
		else
			fb.Text = ""
			delay(1.2, function() fb.Text = "" end)
		end
	end)

	return btn
end

